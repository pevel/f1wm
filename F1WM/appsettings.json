{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Args": {
          "fileSizeLimitBytes": 1000000,
          "path": "logs/",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day"
        },
        "Name": "File"
      },
      {
        "Name": "Console"
      }
    ]
  }
}
